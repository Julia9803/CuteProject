<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-http://www.hibernate.org/dtd/hibernate-mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="PO.GoodsPO">
        <id name="autoId">
            <generator class="native"></generator>
        </id>
        <property name="goodsName"/>
        <property name="goodsID"/>
        <property name="goodsCategory"/>
        <property name="goodsType"/>
        <property name="goodsBuyPrice"/>
        <property name="goodsSellPrice"/>
        <property name="recentBuyPrice"/>
        <property name="recentSellPrice"/>
        <property name="state"/>
    </class>
</hibernate-mapping>