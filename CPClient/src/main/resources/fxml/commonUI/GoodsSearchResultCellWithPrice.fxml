<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<?import javafx.scene.chart.*?>

<HBox maxHeight="25.0" maxWidth="400.0" prefHeight="25.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="id" prefHeight="24.0" prefWidth="47.0" />
      <Label fx:id="name" prefHeight="24.0" prefWidth="55.0" />
      <Label maxHeight="24.0" maxWidth="20.0" minHeight="24.0" minWidth="20.0" prefHeight="24.0" prefWidth="20.0" text="×" />
      <TextField fx:id="amountTextField" maxHeight="25.0" maxWidth="40.0" minHeight="24.0" minWidth="40.0" prefHeight="25.0" prefWidth="40.0" text="1" />
      <Label maxWidth="40.0" minWidth="40.0" prefHeight="24.0" prefWidth="40.0" text="单价" />
      <TextField fx:id="priceTextField" maxHeight="24.0" maxWidth="60.0" minHeight="24.0" minWidth="60.0" prefHeight="24.0" prefWidth="60.0" />
      <Label maxWidth="20.0" minWidth="20.0" prefHeight="24.0" prefWidth="20.0" text="元" />
      <TextField fx:id="notesTextField" maxHeight="24.0" minHeight="24.0" prefHeight="24.0" prefWidth="105.0" promptText="备注">
         <font>
            <Font size="13.0" />
         </font>
      </TextField>
      <CheckBox fx:id="checkBox" mnemonicParsing="false" prefHeight="25.0" prefWidth="16.0" />
   </children>
</HBox>
